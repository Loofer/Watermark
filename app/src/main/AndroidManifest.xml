<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.loofer.watermark"
    android:versionCode="1"
    android:versionName="1.0">


    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme_Light.Default">
        <activity
            android:name="org.loofer.HomeActivity"
            android:label="@string/app_name">
        </activity>

        <activity
            android:name="org.loofer.splash.SplashActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="org.loofer.lead.GuideActivity" />


        <activity android:name="org.loofer.photo.SelectPicActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name="org.loofer.photo.CropActivity" />

        <activity
            android:name="cn.finalteam.rxgalleryfinal.ui.activity.MediaActivity"
            android:exported="true"
            android:theme="@style/Theme_Light.Default" />

        <activity
            android:name="com.yalantis.ucrop.UCropActivity"
            android:theme="@style/Theme_Light.Default"
            android:screenOrientation="portrait" />
        <!--<activity android:name="org.loofer.HomeActivity"/>-->
    </application>

</manifest>